{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Average Prices of Avocado Sold in the US",
  "data": {
    "url": "https://raw.githubusercontent.com/tngu0105/3179/main/avocadoREAL.csv"
  },
  "encoding": {
    "x": {
      "field": "year",
      "timeUnit": "year"
    }
  },
  "layer": [
    {
      "mark": {"type": "errorband", "extent": "ci"},
      "encoding": {
        "y": {
          "field": "AveragePrice",
          "type": "quantitative",
          "title": "Average Price"
        }
      }
    },
    {
      "mark": "line",
      "encoding": {
        "y": {
          "aggregate": "mean",
          "field": "AveragePrice"
        }
      }
    }
  ]

}